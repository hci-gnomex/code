			if (requestCategory == null) {
				return;
			}
			
			requestIcon = requestCategory.@icon;
			requestCategoryName = requestCategory.@display.toString();
			requestType = requestCategory.@type;
			
			if (theTab.contains(notesView)) {
				notesView.setAnalysisVisibility();
