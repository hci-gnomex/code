			public function showAlternateDownloadPage():void {
				var url:URLRequest = null;
				if (parentDocument is NavOrderView) {
					url = new URLRequest('ShowRequestDownloadForm' + 
						(parentApplication.isGuestMode() ? 'ForGuest' : '') +   
						'.gx?idRequest=' + 
						request.@idRequest.toString());
					navigateToURL(url, '_blank');					 
				} else if (parentDocument.selectedIdRequest != null) {
					url = new URLRequest('ShowRequestDownloadForm' + 
						(parentApplication.isGuestMode() ? 'ForGuest' : '') +   
						'.gx?idRequest=' + 
						parentDocument.selectedIdRequest);
					navigateToURL(url, '_blank');					 
				} else if (this.theTab.selectedChild == this.downloadView) {
					var requestNumbers:String = "";
					for each(var dl:Object in this.downloadView.downloadList) {
						if (requestNumbers.length > 0) {
							requestNumbers += ":"
						}
						requestNumbers += dl.@requestNumber;
					}
					if (requestNumbers.length > 0) {
						var url1:URLRequest = new URLRequest('ShowRequestDownloadForm' + 
							(parentApplication.isGuestMode() ? 'ForGuest' : '') +   
							'.gx?requestNumbers=' + 
							requestNumbers);
						navigateToURL(url1, '_blank');					
						
					}
					
				}
			}
