                		  <mx:HRule width="100%"/>
                		  <mx:VBox width="100%" height="100%" verticalGap="0">							
							<mx:Repeater id="requestCategoryRepeater2" dataProvider="{parentApplication.dictionaryManager.xml.Dictionary.(@className == 'hci.gnomex.model.RequestCategory').DictionaryEntry.(@isActive == 'Y' &amp;&amp; @value != ''  != '' &amp;&amp; @isInternal == 'Y' &amp;&amp; @type == parentApplication.TYPE_ILLUMINA)}">
