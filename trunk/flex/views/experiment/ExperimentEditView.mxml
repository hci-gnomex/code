			private function propagateRange():void {
				if((parseInt(rangeFrom.text) <= parseInt(requestApplication.@avgInsertSizeTo)) && 
					(parseInt(rangeFrom.text) >= parseInt(requestApplication.@avgInsertSizeFrom)) && 
					(parseInt(rangeTo.text) <= parseInt(requestApplication.@avgInsertSizeTo)) && 
						(parseInt(rangeTo.text) >= parseInt(requestApplication.@avgInsertSizeFrom))) {
					this.request.@avgInsertSizeFrom = rangeFrom.text;
					this.request.@avgInsertSizeTo = rangeTo.text;
					saveButton.enabled = true;
				}
